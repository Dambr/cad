						cubes.push(new createCube(String(cubeName.value), Number(cubeWidth.value), Number(cubeHeight.value), Number(cubeDepth.value), Number(cubeToLeft.value), Number(cubeToDown.value), Number(cubeRoundX.value), Number(cubeRoundY.value), Number(cubeRoundZ.value), cubeBool.checked, Number(cubeToDepth.value)));

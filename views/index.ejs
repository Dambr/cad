		function checkGeomentry(){
			var n = document.getElementById("geometry").options.selectedIndex;
			var txt = document.getElementById("geometry").options[n].text;
			if (n!=-1 && n!=0){
				if (txt == "Цилиндр"){
					$(".addCubeParams").css("display", "none");
					$(".addFaskParams").css("display", "none");
					$(".addCilindrParams").css("display", "block");
					$(".buttonAdd").css("display", "block");
				}
				if (txt == "Вал с фаской"){
					$(".addCubeParams").css("display", "none");
					$(".addCilindrParams").css("display", "none");
					$(".addFaskParams").css("display", "block");
					$(".buttonAdd").css("display", "block");
				}
				if (txt == "Куб"){
					$(".addCilindrParams").css("display", "none");
					$(".addFaskParams").css("display", "none");
					$(".addCubeParams").css("display", "block");
					$(".buttonAdd").css("display", "block");
				}
			}
		}
		function check(){
			var n = document.getElementById("geometry").options.selectedIndex;
			var txt = document.getElementById("geometry").options[n].text;
			if (n!=-1 && n!=0){
				if (txt == "Цилиндр"){
					for (var jkk1 = 0; jkk1 < cilindrNames.length; jkk1++){
						if (cilindrName.value == cilindrNames[jkk1]){
							alert("Name alredy exists");
							inNames = true;
							break;
						}
					}
					//index, width, height,toleft, todown, roundX, roundY, roundZ
					if (inNames == false){
						cilindrNames.push(cilindrName.value);
						cilindres.push(new createCilindr(String(cilindrName.value), Number(cilindrWidth.value), Number(cilindrHeight.value), Number(cilindrToLeft.value), Number(cilindrToDown.value), Number(cilindrRoundX.value), Number(cilindrRoundY.value), Number(cilindrRoundZ.value), cilindrBool.checked));
					}
					inNames = false;
				}
				if (txt == "Вал с фаской"){
					for (var jkk1 = 0; jkk1 < faskNames.length; jkk1++){
						if (faskName.value == faskNames[jkk1]){
							alert("Name alredy exists");
							inNames = true;
							break;
						}
					}
					//index, width, height,toleft, todown, roundX, roundY, roundZ, where, fask
					if (inNames == false){
						var n1 = document.getElementById("bottop").options.selectedIndex;
						var txt1 = document.getElementById("bottop").options[n1].text;
						if (txt1 != "Выбрать направление"){
							faskNames.push(faskName.value);
							fask.push(new createCilindrWithFask(String(faskName.value), Number(faskWidth.value), Number(faskHeight.value), Number(faskToLeft.value), Number(faskToDown.value), Number(faskRoundX.value), Number(faskRoundY.value), Number(faskRoundZ.value), String(txt1), Number(faskRange.value)));
						}
					}
					inNames = false;
				}
				if (txt == "Куб"){
					for (var jkk1 = 0; jkk1 < cubeNames.length; jkk1++){
						if (cubeName.value == cubeNames[jkk1]){
							alert("Name alredy exists");
